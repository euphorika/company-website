{
  "name": "euphorika-company-website",
  "private": true,
  "description": "Our tiny company website",
  "version": "0.1.0",
  "dependencies": {
    "@mdx-js/mdx": "^1.0.5",
    "@mdx-js/react": "^1.0.2",
    "@mdx-js/tag": "^0.20.3",
    "gatsby": "^2.3.3",
    "gatsby-image": "^2.0.35",
    "gatsby-mdx": "^0.6.2",
    "gatsby-plugin-manifest": "^2.0.25",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-stylus": "^2.0.7",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "rupture": "^0.7.1",
    "scroll-snap": "^1.0.6"
  },
  "devDependencies": {
    "babel-jest": "^24.5.0",
    "babel-preset-gatsby": "^0.1.10",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.6",
    "start-server-and-test": "^1.9.0"
  },
  "keywords": [
    "euphorika",
    "brand page"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "predeploy": "yarn test; CI=1 npm i cypress; yarn test:e2e:ci",
    "deploy": "yarn build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/euphorika/company-website"
  },
  "bugs": {
    "url": "https://github.com/euphorika/company-website/issues"
  }
